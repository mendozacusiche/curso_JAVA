class Empleado{
	
	
	public Empleado(String nombre) {
		
		
		this.nombre = nombre;
		
		sueldo =2000;
	}

	
	public String toString() {
		
		return "[Nombre= "+nombre+", sueldo= "+ sueldo+"]";
	}
	
	
	
	private String nombre;
	
	private double sueldo ;
}













































